{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workflows_SaveText_name": {
      "defaultValue": "SaveText",
      "type": "string"
    },
    "subscription_code": {
      "defaultValue": "/subscriptions/a4f8dcd6-87d0-4b57-a907-1b21b0278071/",
      "type": "string"
    },
    "connections_azuretables_externalid": {
      "defaultValue": "/subscriptions/a4f8dcd6-87d0-4b57-a907-1b21b0278071/resourceGroups/WMMP/providers/Microsoft.Web/connections/azuretables",
      "type": "string"
    },
    "artifactsLocation": {
      "type": "string",
      "defaultValue": "https://raw.githubusercontent.com/Azure/azure-quickstart-templates/master/101-VM-Virus-Attack-Prevention",
    }
  },
  "variables": {
    "templateUri1": "[concat(parameters('artifactsLocation'),'/resources/logicapp.json')]"
  },
  "resources": [
    {
      "type": "Microsoft.Logic/workflows",
      "apiVersion": "2017-07-01",
      "name": "[parameters('workflows_SaveText_name')]",
      "location": "northeurope",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[variables('templateUri1')]"
        }
      }
    }
  ]
}